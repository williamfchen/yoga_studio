<h1><%= @studio.name %> Yogis</h1>
<h3>Studio ID: <%= @studio.id %></h3>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Member?</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  <thead>
  <tbody>
    <% @yogis.each do |yogi| %>
      <tr>
        <td><%= yogi.name %></td>
        <td><%= yogi.age %></td>
        <td><%= yogi.member %></td>
        <td><%= yogi.created_at %></td>
        <td><%= yogi.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>